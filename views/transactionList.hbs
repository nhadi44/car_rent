{{> header}}
{{> hero}}
{{> navadmin}}
<div class="container">
    <div class="row">
        <div class="col">
            <h1 class="fs-4 mb-lg-4">{{title}}</h1>
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Name</th>
                        <th>Car Name</th>
                        <th>Plat Number</th>
                        <th>Type</th>
                        <th>Brand</th>
                        <th>Borrow Date</th>
                        <th>Return Date</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each results}}
                    <tr>
                        <td>{{@index}}</td>
                        <td>{{this.name}}</td>
                        <td>{{this.carname}}</td>
                        <td>{{this.plat_number}}</td>
                        <td>{{this.type}}</td>
                        <td>{{this.brand}}</td>
                        <td>{{this.borrow_date}}</td>
                        <td>{{this.return_date}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>
{{>footer}}